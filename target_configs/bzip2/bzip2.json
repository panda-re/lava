{
    "name": "bzip2",
    "db": "bzip2",
    "preprocessed": true,
    "dataflow": true,

    "tarfile": "bzip2-pre.tar.gz",

    "qcow": "wheezy_panda2.qcow2",
    "snapshot": "root",

{
    "name": "duktape",
    "db": "duktape",
    "preprocessed": true,
    "dataflow": false,

    "tarfile": "duktape-2.3.0-pre.tar.gz",

    "qcow": "wheezy_panda2.qcow2",
    "snapshot": "root",
    "expect_prompt": "root@debian-i386:~#",


    "make": "make",
    "clean": "make clean",
    "install": "make install",
    "command": "{install_dir}/bin/duk {input_file}",
    "COMMENT_injfixupsscript": "duktape_fixup.sh {bug_build}",

    "max_tcn": 10,

    "main_file": [ "src/duktape-pre.c" ],
    "inputs": ["inputs/hello.js"]
}

{
    "name": "toy-va",
    "db": "toy-va",
    "preprocessed": true,
    "dataflow": true,

    "qcow": "wheezy_panda2.qcow2",
    "snapshot": "root",
    "expect_prompt": "root@debian-i386:~#",

    "tarfile": "toy-va-pre.tar.gz",

    "configure": "true",
    "make": "make CFLAGS+=-gdwarf-2",
    "clean": "make clean",
    "install": "make install",
    "command": "{install_dir}/bin/toy-va {input_file}",

    "main_file": [ "toy-va-pre.c" ],

    "inputs": [ "inputs/testsmall.bin" ]
}

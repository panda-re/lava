{
    "name": "tinyexpr",
    "db": "tinyexpr",
    "preprocessed": true,
    "dataflow": true,

    "tarfile": "tinyexpr-pre.tar.gz",

    "qcow": "wheezy_panda2.qcow2",
    "snapshot": "root",
    "expect_prompt": "root@debian-i386:~#",


    "make": "make",
    "install": "make install",
    "command": "{install_dir}/bin/math {input_file}",
    "injfixupsscript_example": "tinyexpr_fixup.sh {bug_build}",

    "max_liveness": 100000,
    "max_cardinality": 100,
    "max_tcn": 10000,
    "max_lval_size": 100,

    "main_file": [ "main-pre.c" ],

    "inputs": ["inputs/input.txt"]
}

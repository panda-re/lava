{
    "name": "sqlite",
    "db": "sqlite",
    "preprocessed": true,
    "dataflow": false,

    "tarfile": "sqlite-pre.tar.gz",

    "qcow": "wheezy_panda2.qcow2",
    "snapshot": "root",
    "expect_prompt": "root@debian-i386:~#",

    "make": "make",
    "clean": "make clean",
    "install": "make install",
    "command": "{install_dir}/bin/sqlite < {input_file}",
    "injfixupsscript_COMMENT": "sqlite_fixup.sh {bug_build}",

    "max_tcn": 10,

    "main_file": [ "src/sqlite3-pre.c" ],
    "inputs": ["inputs/input.sql", "inputs/input2.sql"]
}

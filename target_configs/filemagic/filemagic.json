{
    "name": "filemagic",
    "db": "filemagic",
    "preprocessed": true,
    "dataflow": true,

    "tarfile": "file-5.35-pre.tar.gz",

    "qcow": "wheezy_panda2.qcow2",
    "snapshot": "root",
    "expect_prompt": "root@debian-i386:~#",

    "make": "make",
    "clean": "make clean",
    "install": "make install",
    "command": "{install_dir}/bin/file -m {input_file} {input_file}",
    "injfixupsscript": "file_fixup.sh {bug_build}",

    "max_tcn": 10,

    "main_file": [ "src/file.c" ],

    "inputs": ["inputs/magic.mgc"]
}

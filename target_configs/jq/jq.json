{
    "name": "jq",
    "db": "jq",
    "preprocessed": "true",
    "dataflow": "true",

    "tarfile": "jq-1.6-pre.tar.gz",

    "qcow": "wheezy_panda2.qcow2",
    "snapshot": "root",
    "expect_prompt": "root@debian-i386:~#",

    "make": "make",
    "clean": "make clean",
    "install": "make install",
    "command": "{install_dir}/bin/jq -r . {input_file}",

    "main_file": [ "src/main.c" ],

    "inputs": [ "jq.json" ]
}

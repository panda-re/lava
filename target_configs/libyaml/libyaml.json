{
    "name": "libyaml",
    "db": "libyaml",
    "preprocessed": true,
    "dataflow": false,

    "tarfile": "libyaml-pre.tar.gz",

    "qcow": "wheezy_panda2.qcow2",
    "snapshot": "root",
    "expect_prompt": "root@debian-i386:~#",

    "make": "make",
    "clean": "make clean",
    "install": "make install",
    "command": "{install_dir}/bin/libyaml -c -u {input_file}",

    "COMMENT_injfixupsscript": "fixup.sh {bug_build}",

    "main_file": [ "src/parser.c" ],

    "inputs": [ "inputs/bin.yaml", "inputs/test_yaml2.yaml"]
}
